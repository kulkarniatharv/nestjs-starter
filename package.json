  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prisma:generate": "pnpm exec prisma generate",
    "prisma:migrate:dev": "pnpm exec prisma migrate dev",
    "prisma:migrate:deploy": "pnpm exec prisma migrate deploy",
    "prisma:db:push": "pnpm exec prisma db push",
    "prisma:format": "pnpm exec prisma format",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:seed": "pnpm exec ts-node prisma/seed.ts"
  },
